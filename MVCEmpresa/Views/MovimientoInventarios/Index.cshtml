@model IEnumerable<MVCEmpresa.Models.MovimientoInventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodMovimientoInventario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCatalogoInventarioNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdOrdenNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoMovInventarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodMovimientoInventario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCatalogoInventarioNavigation.IdCatalogoInventario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdOrdenNavigation.IdOrden)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoMovInventarioNavigation.IdTipoMovInventario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMovimientoInventario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMovimientoInventario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMovimientoInventario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
