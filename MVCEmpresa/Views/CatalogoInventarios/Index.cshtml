@model IEnumerable<MVCEmpresa.Models.CatalogoInventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodInventario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstanteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProductoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUbicacionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodInventario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstanteNavigation.IdEstante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProductoNavigation.IdProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUbicacionNavigation.IdUbicacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCatalogoInventario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCatalogoInventario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCatalogoInventario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
